 @article{Damas_1975, title={Three Kinship Systems from the Central Arctic}, volume={12}, ISSN={0066-6939}, abstractNote={The kinship terminologies of the Copper, Netsilik and Iglulik Eskimo show substantial sharing of linguistic content while significant variation occurs in the arrangement of terms. Moving from west to east in the Central Arctic, one can discern an increasing utility for understanding terminology as a system of dyads linked by definite behavioral norms, particularly relating to the axes of supersubordination and closeness-distance, Some of the variations in terminologies can be related to marriage practices while others are anomalous to such explanatory recourse. Extensions of kinship including betrothal and marriage, spouse exchange and adoption find expression in the terminological systems as well. In all three regions extra kinship factors operate alongside kinship and together the two dimensions provide the essential elements of social structure.}, number={1}, journal={Arctic Anthropology}, publisher={University of Wisconsin Press}, author={Damas, David}, year={1975}, pages={10–30} }

